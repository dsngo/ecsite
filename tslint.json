{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "import-name": false,
    "no-unused-variable": true,
    "quotemark": [true, "double", "jsx-double", "avoid-template"],
    "max-line-length": [false, 160],
    "no-var-keyword": false,
    "ter-indent": [false, 2, { "SwitchCase": true }],
    "ter-arrow-parens": false,
    "variable-name": [true, "check-format", "allow-pascal-case", "ban-keywords"],
    "no-this-assignment": [true, {"allowed-names": ["^self$"], "allow-destructuring": true}]
    // "ordered-imports": [
    //   true,
    //   {
    //     "import-sources-order": "lowercase-first",
    //     "named-imports-order": "lowercase-first"
    //   }
    // ],
  }
}
